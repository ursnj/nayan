{
  "name": "website",
  "private": true,
  "description": "Reusable UI Component Libraries for React and React Native.",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "node server",
    "preview": "NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "build:website": "rimraf dist && tsc && npm run build:client && npm run build:server",
    "build:client": "vite build --mode production --outDir dist/client",
    "build:server": "vite build --mode production --outDir dist/server --ssr src/entry-server.jsx",
    "sitemaper": "npx seo-master create sitemap -w http://localhost:7100 -r https://www.nayanui.com -o ./public/sitemap.xml"
  },
  "dependencies": {
    "nayan": "file:../react",
    "react-code-blocks": "^0.1.6",
    "react-helmet-async": "^1.3.0",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "serve-static": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  },
  "license": "ISC"
}
