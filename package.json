{
  "name": "njn-ui",
  "private": false,
  "sideEffects": false,
  "version": "1.0.0-alpha-1",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite",
    "build": "rimraf dist && tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^1.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run format"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ursnj/njn-ui"
  },
  "keywords": [
    "React Component Library",
    "Tailwind CSS",
    "Headless UI",
    "Light & Dark Support",
    "Vite",
    "NextJS"
  ],
  "author": "Niranjan Devasani",
  "bugs": {
    "url": "https://github.com/ursnj/njn-ui/issues"
  },
  "homepage": "https://github.com/ursnj/njn-ui#readme",
  "license": "MIT"
}
